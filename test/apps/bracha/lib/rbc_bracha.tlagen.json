            "global_variables": [
                {
                    "name": "bcNode",
                    "assignment_lines": [
                        "\\in AN"
                    ]
                },
                {
                    "name": "bcValue",
                    "assignment_lines": [
                        "\\in (CASE bcNode \\in CN -> Value [] bcNode \\in FN -> {NotValue})"
                    ]
                },
                {
                    "name": "rbcs",
                    "assignment_lines": [
                        "= [node_id \\in AN |->",
                        "[n |-> N,",
                        "f |-> F,",
                        "me |-> node_id,",
                        "peers |-> AN,",
                        "broadcaster |-> bcNode,",
                        "predicate |-> [v \\in AllValues |-> TRUE],",
                        "max_msg_size |-> 1000,",
                        "propose_sent |-> FALSE,",
                        "msg_recv |-> [msg_from_node_id \\in AN |-> [msg_type \\in MsgTypes |-> FALSE]],",
                        "echo_sent |-> FALSE,",
                        "echo_recv |-> [echo_value \\in AllValues |-> [x \\in AN |-> FALSE]],",
                        "ready_sent |-> FALSE,",
                        "ready_recv |-> [ready_value \\in AllValues |-> [x \\in AN |-> FALSE]],",
                        "output |-> NULL]]"
                    ]
                },
                {
                    "name": "node_input",
                    "assignment_lines": [
                        "= [node_id \\in AN |-> bcValue]"
                    ]
                }
            ],
            "local_variables": [
                {
                    "name": "rbc",
                    "assignment_lines": [
                        "= rbcs[self]"
                    ]
                },
                {
                    "name": "input",
                    "assignment_lines": [
                        "= node_input[self]"
                    ]
                },
                {
                    "name": "msgs",
                    "assignment_lines": [
                        "= [node_id \\in AN |-> {}]"
                    ]
                }
            ],
            "definitions": [
                "AN  == CN \\cup FN",
                "N == Cardinality(AN)",
                "F == Cardinality(FN)",
                "AllValues == Value \\cup {NotValue}",
                "MsgTypes == {\"PROPOSE\", \"ECHO\", \"READY\"}"
